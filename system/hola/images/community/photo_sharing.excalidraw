      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 3585.8350141897886,
      "y": 1648.1002181725905,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "width": 649.3394165039062,
